Bash:
	export APP_SETTINGS="config.DevelopmentConfig"
	export DATABASE_URL="postgresql://localhost:5432/watric_dev"
Windows:
	SET APP_SETTINGS=config.DevelopmentConfig
	SET DATABASE_URL=postgresql://postgres:[LOCAL DEV PASSWORD]@localhost:5432/watric_dev

	SET DATABASE_URL=postgresql://postgres:watricfall@localhost:5432/watric_dev